{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "default": "Data Source=Identity.db;"
  },
  "IdentityServer": {
    "Clients": {
      "BlazorClient": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:7124/authentication/login-callback",
        "LogoutUri": "https://localhost:7124/authentication/login-callback"
      }
    },
    "Key": {
      "Type": "Development"
    }
  }
}
